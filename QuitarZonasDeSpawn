-- Improved script to delete objects using :Destroy()
local workspace = game:GetService("Workspace")

-- Function to safely destroy an object if it exists
local function destroyIfExists(path)
    -- Find the object in the Workspace using the specified path
    local object = workspace:FindFirstChild(path, true)
    if object then
        -- Destroy the object
        object:Destroy()
    end
end

-- List of object paths to delete
local objectsToDestroy = {
    "City/Buildings",
    "City/Interactive/Bank/Model",
    "City/Interactive/Police Station", -- Consider handling specific children if necessary
    "City/Interactive/Grove/WareHouse",
    "City/Interactive/Main Plaza", -- Consider handling specific children if necessary
    "SafeZones/Barrier",
}

-- Iterate through the list and destroy each object
for _, path in ipairs(objectsToDestroy) do
    destroyIfExists(path)
end